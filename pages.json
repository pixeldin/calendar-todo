{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			// "path": "pages/index/index",
			"path": "pages/zui-calendar/components/zui-calendar/zui-calendar",
			"style": {
				"navigationStyle": "custom",
				"onReachBottomDistance": 70,
				// "navigationBarTitleText": "pixel-todo",
				"navigationBarBackgroundColor": "#FCEDFA",
				"app-plus": {
					"titleNView": false, //禁用原生导航栏
					"bounce": "none", //关闭窗口回弹效果
					"titleNView": {
						"backButton": {
							"color": "#ffffff",
							"fontSize": "32px"
						}
					}
				}
			}
		}, {
			"path": "pages/zui-calendar/components/zui-calendar/TabSwitch/TabSwitch",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/zui-calendar/components/zui-calendar/newItem/newItem",
			"style": {				
				"navigationStyle": "custom",
				"onReachBottomDistance": 70,
				"app-plus": {
					"titleNView": false, //禁用原生导航栏
					"bounce": "none", //关闭窗口回弹效果
					"titleNView": {
						"backButton": {
							"color": "#ffffff",
							"fontSize": "32px"
						}
					}
				}
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
